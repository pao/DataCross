<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.greentaperacing.racelog"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".RaceLog"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DownVectorCalibration"
                  android:label="@string/app_name">
        <intent-filter><action android:name="com.greentaperacing.racelog.DOWN_VECTOR_CALIBRATION"></action>
</intent-filter>
</activity>
        <activity android:name=".HeadingCalibration"
                  android:label="@string/app_name">
        <intent-filter><action android:name="com.greentaperacing.racelog.HEADING_CALIBRATION"></action>
</intent-filter>
</activity>
        <activity android:name=".TrackTrajectory"
                  android:label="@string/app_name">
        <intent-filter><action android:name="com.greentaperacing.racelog.TRACK_TRAJECTORY"></action>
</intent-filter>
</activity>
    </application>
    <uses-sdk android:minSdkVersion="6" />

<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
</manifest> 