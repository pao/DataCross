<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.greentaperacing.datacross"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name="com.greentaperacing.racelog.DataCross"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.greentaperacing.racelog.DownVectorCalibration"
                  android:label="@string/app_name">
        <intent-filter><action android:name="com.greentaperacing.racelog.DOWN_VECTOR_CALIBRATION"></action>
<category android:name="android.intent.category.DEFAULT"></category>
</intent-filter>
</activity>
        <activity android:name="com.greentaperacing.racelog.HeadingCalibration"
                  android:label="@string/app_name">
        <intent-filter><action android:name="com.greentaperacing.racelog.HEADING_CALIBRATION"></action>
<category android:name="android.intent.category.DEFAULT"></category>
</intent-filter>
</activity>
        <activity android:name="com.greentaperacing.racelog.TrackTrajectory"
                  android:label="@string/app_name">
        <intent-filter><action android:name="com.greentaperacing.racelog.TRACK_TRAJECTORY"></action>
<category android:name="android.intent.category.DEFAULT"></category>
</intent-filter>
</activity>
        <uses-library android:name="com.google.android.maps" android:required="true"></uses-library>
    </application>
    <uses-sdk android:minSdkVersion="8" />

<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
</manifest> 